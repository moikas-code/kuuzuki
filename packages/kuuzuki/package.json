{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.1.19",
  "name": "kuuzuki",
  "description": "AI-powered terminal assistant",
  "keywords": [
    "ai",
    "terminal",
    "cli",
    "assistant",
    "claude"
  ],
  "homepage": "https://github.com/moikas-code/kuuzuki",
  "repository": {
    "type": "git",
    "url": "https://github.com/moikas-code/kuuzuki.git"
  },
  "bugs": {
    "url": "https://github.com/moikas-code/kuuzuki/issues"
  },
  "author": "Kuuzuki Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "bun run ./src/index.ts",
    "build": "node scripts/build-bun-cli.js",
    "build:bun": "node scripts/build-bun-cli.js",
    "build:bun-api": "bun run scripts/build-bun.js",
    "build:tsc": "node scripts/build-typescript.js && mv dist dist-tsc",
    "build:watch": "tsc --watch",
    "build:exe": "node scripts/build-executable.js",
    "build:all": "npm run build:bun && npm run build:tsc",
    "validate": "node scripts/validate-package.js",
    "prepublishOnly": "npm run validate && node scripts/prepublish.js",
    "postinstall": "node scripts/postinstall.js",
    "parity:generate": "cd ../.. && fork-parity-dashboard generate",
    "parity:analyze": "cd ../.. && fork-parity-dashboard analyze",
    "parity:dashboard": "cd ../.. && fork-parity-dashboard serve",
    "parity:cleanup": "cd ../.. && fork-parity-dashboard cleanup",
    "parity:sync": "cd ../.. && fork-parity sync && npm run parity:generate",
    "parity:status": "cd ../.. && fork-parity dashboard --format=markdown",
    "parity:critical": "cd ../.. && fork-parity dashboard --priority=critical --format=markdown"
  },
  "bin": {
    "kuuzuki": "./bin/kuuzuki.js"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@ai-sdk/amazon-bedrock": "2.2.10",
    "@ai-sdk/anthropic": "1.2.12",
    "@octokit/webhooks-types": "7.6.1",
    "@tsconfig/bun": "1.0.7",
    "@types/bun": "latest",
    "@types/turndown": "5.0.5",
    "@types/yargs": "17.0.33",
    "sst": "3.17.10",
    "typescript": "5.8.0-dev.20250106",
    "vscode-languageserver-types": "3.17.5",
    "zod-to-json-schema": "3.24.5"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "dist-tsc/**/*",
    "binaries/**/*",
    "scripts/**/*",
    "README.md"
  ],
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1",
    "@clack/prompts": "0.11.0",
    "@octokit/graphql": "8.1.2",
    "@octokit/rest": "22.0.0",
    "@hono/zod-validator": "0.4.2",
    "@modelcontextprotocol/sdk": "1.15.1",
    "@openauthjs/openauth": "0.4.3",
    "@standard-schema/spec": "1.0.0",
    "@zip.js/zip.js": "2.7.62",
    "ai": "5.0.0-beta.34",
    "chalk": "5.4.1",
    "decimal.js": "10.5.0",
    "diff": "8.0.2",
    "gray-matter": "4.0.3",
    "hono": "4.7.10",
    "hono-openapi": "0.4.8",
    "isomorphic-git": "1.32.1",
    "open": "10.2.0",
    "remeda": "2.22.3",
    "tree-sitter": "^0.20.0",
    "tree-sitter-bash": "^0.20.0",
    "turndown": "7.2.0",
    "vscode-jsonrpc": "8.2.1",
    "xdg-basedir": "5.1.0",
    "yargs": "18.0.0",
    "zod": "3.25.49",
    "zod-openapi": "4.1.0"
  }
}
